# Build system imports
import libs

# Copy Our Build Environment
Import('env')
envl = env.Clone()

sources = ['src/BasicImage.cpp',
           'src/OpenCVCamera.cpp',
           'src/OpenCVImage.cpp']

# Pull in opencv
libs.add_external('OpenCV', envl)

# Base Vision Library
envl.SharedLibrary(target = 'ram_vision', source = sources)
