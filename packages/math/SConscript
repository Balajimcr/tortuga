# Build system imports
import libs
import helpers

# Copy Our Build Environment
Import('env')
envl = env.Clone()

# Sources
sources = helpers.glob(env, 'src', '*.cpp')

# Build Procducts (library will be called 'libram_math.so')
helpers.SharedLibrary(envl, 'math',  source = sources)

